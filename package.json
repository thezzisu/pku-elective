{
  "name": "pku-elective",
  "version": "1.0.0",
  "description": "PKU Elective",
  "main": "./out/main/index.js",
  "author": "thezzisu",
  "homepage": "https://zisu.dev",
  "license": "AGPL-3.0",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "yarn run typecheck:node && yarn run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "yarn run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "yarn run build && electron-builder --win --config",
    "build:mac": "yarn run build && electron-builder --mac --config",
    "build:linux": "yarn run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@fastify/cors": "^8.2.0",
    "electron-updater": "^5.3.0",
    "pku-elective-lib": "^0.1.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@mdi/font": "^7.1.96",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.18.11",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vueuse/core": "^9.13.0",
    "electron": "^21.3.3",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.17",
    "eslint": "^8.31.0",
    "eslint-plugin-vue": "^9.8.0",
    "less": "^4.1.3",
    "prettier": "^2.8.2",
    "roboto-fontface": "^0.10.0",
    "sass": "^1.58.3",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-vuetify": "^1.0.2",
    "vite-plugin-windicss": "^1.8.10",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.0.22",
    "vuetify": "^3.1.5",
    "webfontloader": "^1.6.28"
  }
}
